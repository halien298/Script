if not game:IsLoaded() then game.Loaded:Wait() end task.wait(1)
local Library=loadstring(game:HttpGet("https://raw.githubusercontent.com/vineckociganek/linoria-lib-gui/refs/heads/main/ui"))()
Library.AccentColor=Color3.fromRGB(80,0,100) Library.OutlineColor=Color3.fromRGB(30,30,30) Library:UpdateColorsUsingRegistry()
local Window=Library:CreateWindow({Title="ðŸ’œIdiotÂ´s Playground - State of AnarchyðŸ’œ",Center=true,AutoShow=true})
local Players=game:GetService("Players") local RunService=game:GetService("RunService") local UserInputService=game:GetService("UserInputService")
local LocalPlayer=Players.LocalPlayer local Camera=workspace.CurrentCamera local Mouse=LocalPlayer:GetMouse()
Library:SetWatermarkVisibility(true)
RunService.Heartbeat:Connect(function() Library:SetWatermark(string.format("ðŸ’œState of AnarchyðŸ’œ - Rage | %s | %s",LocalPlayer.Name,os.date("%H:%M:%S"))) end)
local Settings={SilentAimEnabled=false,FOVRadius=150,TeamCheck=true,AimPart="Head"}
local RageTab=Window:AddTab("rage") local VisualsTab=Window:AddTab("visuals") local MiscTab=Window:AddTab("misc")
local HitboxGroup=RageTab:AddLeftGroupbox("head hitbox") HitboxGroup:AddLabel("need fix")
local bigHeadToggle=HitboxGroup:AddToggle("BigHeadHitbox",{Text="hitbox",Default=false})
HitboxGroup:AddSlider("HeadHitboxSize",{Text="size",Min=1,Max=300,Default=4.9,Rounding=1})
local ExploitsGroup=RageTab:AddRightGroupbox("exploits")
local silentAimToggle=ExploitsGroup:AddToggle("SilentAim",{Text="bulletbug",Default=false,Callback=function(v)Settings.SilentAimEnabled=v end})
ExploitsGroup:AddLabel("Bulletbug key"):AddKeyPicker("SilentKey",{Default="F",NoUI=true})
ExploitsGroup:AddLabel("Big Head key"):AddKeyPicker("BigHeadKey",{Default="G",NoUI=true})
local ESPGroup=VisualsTab:AddLeftGroupbox("esp")
ESPGroup:AddToggle("EspBox",{Text="box"}) ESPGroup:AddToggle("EspHealth",{Text="health"}) ESPGroup:AddToggle("EspName",{Text="name"})
ESPGroup:AddToggle("EspDistance",{Text="distance"}) ESPGroup:AddToggle("EspTracer",{Text="tracer"})
local MoveGroup=MiscTab:AddLeftGroupbox("movement") local SafetyGroup=MiscTab:AddLeftGroupbox("safety")
local TimeGroup=MiscTab:AddRightGroupbox("time") local MenuGroup=MiscTab:AddRightGroupbox("menu")
local Lighting=game:GetService("Lighting") local isTimeFrozen=false local targetTime=12
local function forceSetTime(hours) hours=hours%24 local h=math.floor(hours) local m=math.floor((hours-h)*60) local s=math.floor(((hours-h)*60-m)*60)
Lighting.TimeOfDay=string.format("%02d:%02d:%02d",h,m,s) Lighting.ClockTime=hours end
RunService.Heartbeat:Connect(function() if isTimeFrozen then forceSetTime(targetTime) end end)
TimeGroup:AddToggle("FreezeTime",{Text="Freeze Time",Default=false,Callback=function(v) isTimeFrozen=v
if v then forceSetTime(targetTime) Library:Notify("Time frozen",3) else Library:Notify("Time unfrozen",3) end end})
TimeGroup:AddSlider("TimeValue",{Text="Time (hours)",Min=0,Max=24,Default=12,Rounding=1,Increment=0.1,Callback=function(v)
targetTime=v if isTimeFrozen then forceSetTime(v) end end})
MoveGroup:AddButton("tp to spawn",function() local char=LocalPlayer.Character if not char then return end
local hrp=char:FindFirstChild("HumanoidRootPart") if not hrp then return end local spawns=workspace:FindFirstChild("PlayerSpawns") if not spawns then return end
for _,v in ipairs(spawns:GetDescendants()) do if v:IsA("BasePart") then hrp.CFrame=v.CFrame+Vector3.new(0,3,0) break end end end)
local SPEED_VALUE=16 local isSpeedEnabled=false local isFlying=false local flySpeed=30
MoveGroup:AddToggle("SpeedToggle",{Text="speed hack",Default=false,Callback=function(v)isSpeedEnabled=v end})
local flyToggle=MoveGroup:AddToggle("FlyToggle",{Text="fly hack",Default=false,Callback=function(v)
isFlying=v local char=LocalPlayer.Character if not char then return end local hrp=char:FindFirstChild("HumanoidRootPart") if not hrp then return end
if v then local bv=Instance.new("BodyVelocity") bv.MaxForce=Vector3.new(math.huge,math.huge,math.huge) bv.Velocity=Vector3.new(0,0,0) bv.Name="FlyVelocity" bv.Parent=hrp
local bg=Instance.new("BodyGyro") bg.MaxTorque=Vector3.new(math.huge,math.huge,math.huge) bg.CFrame=hrp.CFrame bg.Name="FlyGyro" bg.Parent=hrp
else if hrp:FindFirstChild("FlyVelocity")then hrp.FlyVelocity:Destroy()end if hrp:FindFirstChild("FlyGyro")then hrp.FlyGyro:Destroy()end end end})
MoveGroup:AddSlider("SpeedValue",{Text="walk speed",Default=16,Min=16,Max=48,Rounding=0,Callback=function(v)SPEED_VALUE=v end})
MoveGroup:AddSlider("FlySpeed",{Text="fly speed",Default=30,Min=10,Max=48,Rounding=0,Callback=function(v)flySpeed=v end})
MoveGroup:AddLabel("Fly key"):AddKeyPicker("FlyKey",{Default="V",NoUI=true})
RunService.Heartbeat:Connect(function()
if not isSpeedEnabled then return end local char=LocalPlayer.Character if not char then return end
local hum=char:FindFirstChildOfClass("Humanoid") local root=char:FindFirstChild("HumanoidRootPart")
if not hum or not root or hum.Health<=0 then return end local moveDir=hum.MoveDirection
if moveDir.Magnitude>0.01 then local wishVel=moveDir*SPEED_VALUE
root.AssemblyLinearVelocity=Vector3.new(wishVel.X,root.AssemblyLinearVelocity.Y,wishVel.Z)
if hum:GetState()==Enum.HumanoidStateType.Running then root.AssemblyLinearVelocity+=Vector3.new(0,0.4,0) end end end)
RunService.RenderStepped:Connect(function()
if not isFlying then return end local char=LocalPlayer.Character if not char then return end
local hrp=char:FindFirstChild("HumanoidRootPart") local bv=hrp and hrp:FindFirstChild("FlyVelocity")
local bg=hrp and hrp:FindFirstChild("FlyGyro") if not bv or not bg then return end
local moveDir=Vector3.new(0,0,0)
if UserInputService:IsKeyDown(Enum.KeyCode.W)then moveDir+=Camera.CFrame.LookVector end
if UserInputService:IsKeyDown(Enum.KeyCode.S)then moveDir-=Camera.CFrame.LookVector end
if UserInputService:IsKeyDown(Enum.KeyCode.A)then moveDir-=Camera.CFrame.RightVector end
if UserInputService:IsKeyDown(Enum.KeyCode.D)then moveDir+=Camera.CFrame.RightVector end
if UserInputService:IsKeyDown(Enum.KeyCode.Space)then moveDir+=Vector3.new(0,1,0)end
if UserInputService:IsKeyDown(Enum.KeyCode.LeftControl)then moveDir-=Vector3.new(0,1,0)end
if moveDir.Magnitude>0 then moveDir=moveDir.Unit end bv.Velocity=moveDir*flySpeed bg.CFrame=Camera.CFrame end)
local oldNamecall oldNamecall=hookmetamethod(game,"__namecall",function(self,...)
local method=getnamecallmethod() local args={...}
if Settings.SilentAimEnabled and method=="FireServer" and not checkcaller() then
local closest,minDist=nil,Settings.FOVRadius
for _,plr in Players:GetPlayers() do
if plr==LocalPlayer then continue end
if Settings.TeamCheck and plr.Team==LocalPlayer.Team then continue end
local char=plr.Character if not char or not char:FindFirstChild(Settings.AimPart)then continue end
local part=char[Settings.AimPart] local screen,vis=Camera:WorldToViewportPoint(part.Position)
if not vis then continue end local mousePos=Vector2.new(Mouse.X,Mouse.Y)
local dist=(Vector2.new(screen.X,screen.Y)-mousePos).Magnitude
if dist<minDist then minDist=dist closest=part end end
if closest then
if typeof(args[1])=="Instance" then args[1]=closest end
if typeof(args[2])=="Vector3" or typeof(args[2])=="CFrame" then args[2]=closest.Position end end end
return oldNamecall(self,unpack(args)) end)
SafetyGroup:AddToggle("KickOnUser",{Text="kick on admin join",Default=false})
local BlacklistedIds={5111810890,1535447641,1064972369,1463162055,968686896,136578775,6176229663,629102359,2435868107,282212831,8894067}
local function IsBlacklisted(plr) for _,id in ipairs(BlacklistedIds)do if plr.UserId==id then return true end end return false end
local function KickCheck(plr) if Library.Toggles.KickOnUser.Value and IsBlacklisted(plr)then LocalPlayer:Kick("na serveru objeveny jebanÃ½ mudak")end end
for _,p in Players:GetPlayers()do KickCheck(p)end Players.PlayerAdded:Connect(KickCheck)
MenuGroup:AddLabel("menu key"):AddKeyPicker("MenuKeybind",{Default="Insert",NoUI=true})
Library.ToggleKeybind=Library.Options.MenuKeybind
MenuGroup:AddButton("unload",function()
for _,esp in pairs(ESP or {})do pcall(function() esp.Box:Remove() esp.Name:Remove() esp.Distance:Remove() esp.Health:Remove() esp.Tracer:Remove() end)end
ESP={} pcall(function()Library:Unload()end) print("[NiggaHack] Unloaded") end)
local OriginalHeadSizes={} local HeadHighlights={}
local function ApplyHeadHitbox(char)
local head=char and char:FindFirstChild("Head") if not head or not head:IsA("BasePart")then return end
if Library.Toggles.BigHeadHitbox.Value then
OriginalHeadSizes[char]=OriginalHeadSizes[char]or head.Size local s=Library.Options.HeadHitboxSize.Value
head.Size=Vector3.new(s,s,s) head.Transparency=0.8
if not HeadHighlights[char]then local hl=Instance.new("Highlight")
hl.Adornee=head hl.FillColor=Color3.fromRGB(170,0,255) hl.OutlineColor=Color3.new(1,1,1)
hl.FillTransparency=0.4 hl.OutlineTransparency=0 hl.DepthMode=Enum.HighlightDepthMode.AlwaysOnTop hl.Parent=char
HeadHighlights[char]=hl end
else if OriginalHeadSizes[char]then head.Size=OriginalHeadSizes[char] head.Transparency=0 end
if HeadHighlights[char]then HeadHighlights[char]:Destroy() HeadHighlights[char]=nil end end end
local function HookHitbox(plr) if plr==LocalPlayer then return end
plr.CharacterAdded:Connect(function(c)task.wait(0.3)ApplyHeadHitbox(c)end)
if plr.Character then ApplyHeadHitbox(plr.Character)end end
for _,plr in Players:GetPlayers()do HookHitbox(plr)end Players.PlayerAdded:Connect(HookHitbox)
Library.Toggles.BigHeadHitbox:OnChanged(function()
for _,plr in Players:GetPlayers()do if plr~=LocalPlayer and plr.Character then ApplyHeadHitbox(plr.Character)end end end)
Library.Options.HeadHitboxSize:OnChanged(function()
if not Library.Toggles.BigHeadHitbox.Value then return end
for _,plr in Players:GetPlayers()do if plr~=LocalPlayer and plr.Character then ApplyHeadHitbox(plr.Character)end end end)
local function Square()local d=Drawing.new("Square")d.Visible=false d.Filled=false d.Thickness=1 d.Color=Color3.fromRGB(170,0,255)return d end
local function Text()local d=Drawing.new("Text")d.Visible=false d.Center=true d.Outline=true d.Size=13 d.Color=Color3.new(1,1,1)return d end
local function Line()local d=Drawing.new("Line")d.Visible=false d.Thickness=1.8 d.Color=Color3.fromRGB(170,0,255)d.Transparency=1 return d end
local ESP={}
local function AddPlayer(plr)if plr==LocalPlayer then return end
ESP[plr]={Box=Square(),Name=Text(),Distance=Text(),Health=Line(),Tracer=Line()}end
local function RemovePlayer(plr)if ESP[plr]then for _,v in pairs(ESP[plr])do pcall(function()v:Remove()end)end ESP[plr]=nil end end
for _,p in Players:GetPlayers()do AddPlayer(p)end Players.PlayerAdded:Connect(AddPlayer) Players.PlayerRemoving:Connect(RemovePlayer)
local bulletKey=Enum.KeyCode.F local bigHeadKey=Enum.KeyCode.G local flyKey=Enum.KeyCode.V
Library.Options.SilentKey:OnChanged(function(new)bulletKey=new or Enum.KeyCode.F end)
Library.Options.BigHeadKey:OnChanged(function(new)bigHeadKey=new or Enum.KeyCode.G end)
Library.Options.FlyKey:OnChanged(function(new)flyKey=new or Enum.KeyCode.V end)
UserInputService.InputBegan:Connect(function(input,gameProcessed)
if gameProcessed then return end
if input.KeyCode==bulletKey then local newState=not Settings.SilentAimEnabled Settings.SilentAimEnabled=newState
silentAimToggle:SetValue(newState) Library:Notify("Bulletbug "..(newState and "ON"or"OFF"),2) end
if input.KeyCode==bigHeadKey then local newState=not Library.Toggles.BigHeadHitbox.Value
bigHeadToggle:SetValue(newState) Library:Notify("Big Head Hitbox "..(newState and "ON"or"OFF"),2) end
if input.KeyCode==flyKey then local newFly=not isFlying isFlying=newFly flyToggle:SetValue(newFly)
Library:Notify("Fly Hack "..(newFly and "ON"or"OFF"),2) end end)
RunService.RenderStepped:Connect(function()
local cam=workspace.CurrentCamera
for plr,esp in pairs(ESP)do local char=plr.Character local hum=char and char:FindFirstChildOfClass("Humanoid")
if not(char and hum and hum.Health>0)then for _,v in pairs(esp)do pcall(function()v.Visible=false end)end continue end
local minX,minY=math.huge,math.huge local maxX,maxY=-math.huge,-math.huge local onScreen=false
for _,part in ipairs(char:GetChildren())do
if part:IsA("BasePart")or part:IsA("MeshPart")then local p,vis=cam:WorldToViewportPoint(part.Position)
if vis then onScreen=true minX=math.min(minX,p.X)minY=math.min(minY,p.Y)maxX=math.max(maxX,p.X)maxY=math.max(maxY,p.Y)end end end
if not onScreen then for _,v in pairs(esp)do pcall(function()v.Visible=false end)end continue end
local cx=(minX+maxX)/2 local boxW=maxX-minX local boxH=maxY-minY
esp.Box.Visible=Library.Toggles.EspBox.Value esp.Box.Position=Vector2.new(minX,minY) esp.Box.Size=Vector2.new(boxW,boxH)
esp.Name.Visible=Library.Toggles.EspName.Value esp.Name.Text=plr.Name esp.Name.Position=Vector2.new(cx,minY-16)
esp.Distance.Visible=Library.Toggles.EspDistance.Value
local hrp=char:FindFirstChild("HumanoidRootPart")
local dist=hrp and (cam.CFrame.Position-hrp.Position).Magnitude or 9999
esp.Distance.Text=math.floor(dist).."m" esp.Distance.Position=Vector2.new(cx,maxY+4)
esp.Tracer.Visible=Library.Toggles.EspTracer.Value esp.Tracer.From=Vector2.new(cam.ViewportSize.X/2,cam.ViewportSize.Y)
esp.Tracer.To=Vector2.new(cx,maxY)
esp.Health.Visible=Library.Toggles.EspHealth.Value
if esp.Health.Visible then local hpPct=math.clamp(hum.Health/hum.MaxHealth,0,1)
esp.Health.From=Vector2.new(minX-6,maxY) esp.Health.To=Vector2.new(minX-6,maxY-boxH*hpPct)
esp.Health.Color=Color3.new(1-hpPct,hpPct,0) esp.Health.Thickness=2.5 end end end)
print("[NiggaHack] Loaded successfully!")
